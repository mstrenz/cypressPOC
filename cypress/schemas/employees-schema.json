    "properties": {
      "id": {
        "$id": "/properties/id",
        "type": "integer",
        "title": "The Id Schema ",
        "default": 0,
        "examples": [
          1
        ]
      },
      "first_name": {
        "$id": "/properties/first_name",
        "type": "string",
        "title": "The First_name Schema ",
        "default": "",
        "examples": [
          "Tom"
        ]
      },
      "last_name": {
        "$id": "/properties/last_name",
        "type": "string",
        "title": "The Last_name Schema ",
        "default": "",
        "examples": [
          "Taylor"
        ]
      },
      "email": {
        "$id": "/properties/email",
        "type": "string",
        "title": "The Email Schema ",
        "default": "",
        "examples": [
          "tomt@slalom.com"
        ]
      },
      "location": {
        "$id": "/properties/location",
        "type": "integer",
        "title": "The Location Schema ",
        "default": 0,
        "examples": [
          1
        ]
      }
    }
